#!/bin/sh

pstree -TAh |
             sed 's/|//g;s/---/\n/g;s/ //g;s/`//g;s/^-/\n/g;s/+/\n/g;s/[0-9]\*\[//g;s/]//g' |\
             sed 's/^-//g;s/-$//g'  |\
             sort -u |\
             tail -n +7 |\
             rofi -no-lazy-grab -theme ~/.config/rofi/launchers-git/blurry-dmenu.rasi -dmenu -p "Processes"
                 

